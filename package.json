{
  "name": "lazy-js",
  "version": "0.0.1",
  "description": "Lazy instruments for functions",
  "main": "src/lazy.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start",
    "test-bin": "./node_modules/.bin/karma start karma-bin.conf.js",
